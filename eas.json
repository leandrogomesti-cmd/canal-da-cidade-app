{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true
    },
    "internal": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "withoutCredentials": true
      },
      "distribution": "internal",
      "autoIncrement": true
    }
  }
}
